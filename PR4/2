<body>   
    <input id="coef1" type="text" value="1">
    <input id="coef2" type="text" value="-5">
    <input id="coef3" type="text" value="4">
    <input type="button" onclick="FindRoot(coef1.value, coef2.value, coef3.value)">
    <script>
        let coef1 = document.querySelector('#coef1');
        let coef2 = document.querySelector('#coef2');
        let coef3 = document.querySelector('#coef3');
        let dis, result;
        function FindRoot(a, b, c){
            let dis, result = [0, 0];
            dis = b * b - 4 * a * c;
            if(dis < 0) alert("No roots");
            else if(dis == 0){
                result[0] = (-1 * b) / (2 * a);
                alert(result[0]);
            }
            else{
                result[0] = (-1 * b + Math.sqrt(dis)) / (2 * a);
                result[1] = (-1 * b - Math.sqrt(dis)) / (2 * a);
                alert(result);
            }
        }
    </script>
